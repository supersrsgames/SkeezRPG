package actor {	import engine.WeaponItem;	import engine.InventoryItem;	/**	 * @author alexmccormack	 */	public class QuestData {		private var questData:Array = new Array();		private var voidItem:InventoryItem = new InventoryItem();		private var voidWeapon:WeaponItem = new WeaponItem("void");		private var weaponData:Array = new Array();		public function QuestData(){			genWeapons();			genQuests();		}		public function getQuest(_id:Number):Quest{			var quest:Quest = new Quest(questData[_id][0], questData[_id][1], questData[_id][2], questData[_id][3], questData[_id][4], questData[_id][5], questData[_id][6], questData[_id][7]);			return(quest);		}		private function genQuests():void{			questData[0] = ["collect", true, false, false, 100, voidItem, voidWeapon,100];			questData[1] = ["kill",true,false,true,50,voidItem,weaponData[0],30];		}		private function genWeapons():void{			weaponData[0] = new WeaponItem("axe",100);		}	}}